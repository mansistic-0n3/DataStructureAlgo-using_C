#include <stdio.h>
#include <stdlib.h>
struct node{
    int data;
    struct node *next;
    struct node *prev;
};                      
int main() {
    int choice=1;
    struct node *newnode, *start= NULL, *temp;
    while(choice){
    newnode = (struct node*) malloc (sizeof (struct node));
    printf("Enter data: ");
    scanf("%d", &newnode -> data);
    newnode -> prev = NULL;
    newnode -> next = NULL;
    if(start == NULL){
        temp=newnode;
        start=newnode;
    }else{
        temp -> next = newnode;
        newnode -> prev = temp;
        temp = newnode;
    }
     printf("Do you want to add more nodes(1/0): ");
     scanf("%d", &choice);
    }
    temp=start;
    while (temp != NULL)
    {
        printf("Data is %d \n", temp -> data);
        temp= temp -> next;
    }
}
